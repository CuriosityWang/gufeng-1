.syh-article-contents .text-item{margin:8px 0; line-height:2.2em; font-size:16px; color: #d6d6d6;}
.syh-article-browse{color: #8F8F94; font-size: 10px; font-weight: 100; margin-top: 20px;}
.syh-article-download{float: left; margin-left: 20px; width: 50%;}
.syh-copy-btn{color: #D6D6D6; background-color: #303030; height: 40px; width: 150px; line-height: 20px; text-align: center; margin-top: 15px; border-radius: 10px;}
.content rich-text{padding: 10px;font-size: 35rpx;color: #D6D6D6;line-height: 50rpx;}
.syh-write_title{width: 70%; height: 60px; text-align: center; margin-top: 220rpx;float: left; margin-left: 20px;}
.syh-write_title input{width:100%; font-size:20px; line-height:26px; text-align:center; border-bottom:1px solid #F1F2F3;}
.syh-write_title-2{width: 100%; height: 60px; text-align: center;}
.syh-write_title-2 view{width: 30%;font-size:20px; line-height:26px;float: left;}
.syh-write_title-2 input{width:60%; font-size:20px; line-height:26px; text-align:left; float: left; border-bottom:1px solid #F1F2F3; margin-top: 2px;}
.syh-icon{height: 60px; width: 60px; overflow: hidden; border-radius: 10px; margin-top: 150rpx;float: left; margin-left: 10px;}
.syh-icon image{width: 100%;}
.login-tx{width: 100%;height:200px; position: relative;}
.login-tx image{width: 120px; position: absolute; left:50%; top: 50%; margin-left: -60px; margin-top: -20px;}
.login-text{text-align: center; margin-top: 20px; color: #D6D6D6; font-size: 24px;}
.login-text view{font-size: 12px; color: #8F8F94; margin-top: 10px;}
.login-button-wx{width: 250px; border-radius: 50px; height: 50px; line-height: 50px; margin-top: 20px; background-color: #00B26A;}
.login-button-wx-father{margin-left: 43px;}
.login-button-wx-father view{float: left; margin-left: 10px; color: #FFFFFF;}
.login-button-wx-father image{float: left; width: 30px; margin-top: 12px;}
.login-button-qq{width: 250px; border-radius: 50px; height: 50px; line-height: 50px; margin-top: 20px; background-color: #007AFF;}
.login-button-qq-father{margin-left: 50px;}
.login-button-qq-father view{float: left; margin-left: 10px; color: #FFFFFF;}
.login-button-qq-father image{float: left; width: 25px; margin-top: 10px;}
.syh-login{padding:10px; border-bottom:1px dashed #303030; width: 300px; margin: 10px auto;}
.syh-login input{width:100%; font-size:20px; line-height:26px; text-align:center; color: #D6D6D6;}
.login-button-w{width: 250px; border-radius: 50px; height: 50px; line-height: 50px; margin-top: 20px; background-color: #a337ad;}
.login-button-w-father{margin-left: 77px;}
.login-button-w-father view{float: left; margin-left: 10px; color: #FFFFFF;}
.login-button-w-father image{float: left; width: 20px; margin-top: 15px;}
.syh-register{color: #8F8F94; position: absolute; right: 50px; margin-top: 20px;}
.syh-register-title{color: #D6D6D6; font-size: 24px; text-align: center;}
.syh-tx-show{margin: 0 auto; width: 100px; height: 100px; overflow: hidden;border-radius: 100%; margin-top: 20px;}
.syh-tx-show image{width: 100%}
.syh-sc{color: #09BB07; font-size: 16px; text-align: center; margin-top: 10px;}

